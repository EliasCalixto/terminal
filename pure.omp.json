{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#97e3bf",
          "powerline_symbol": "",
          "style": "powerline",
          "template": "({{ .Venv }}) ",
          "type": "python"
        },
        {
          "foreground": "#ff7280",
          "style": "plain",
          "template": "darkesthj ",
          "type": "session"
        },
        {
          "foreground": "#89c8df",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#6C6C6C",
          "properties": {
            "branch_ahead_icon": "<#88C0D0>\u21e1 </>",
            "branch_behind_icon": "<#88C0D0>\u21e3 </>",
            "branch_icon": "",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "github_icon": ""
          },
          "style": "plain",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}<#ff7280>*</>{{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}*{{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}\ue235{{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#92c089",
          "properties": {
            "style": "austin"
          },
          "style": "plain",
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#8e98b4",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#BF616A{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u276f ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "darkesthj",
  "transient_prompt": {
    "foreground": "#8e98b4",
    "foreground_templates": [
      "{{ if gt .Code 0 }}#BF616A{{ end }}"
    ],
    "template": "\u276f "
  },
  "version": 2
}
